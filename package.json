{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "prepare": "husky install",
    "test": "prettier --check . && eslint . && stylelint \"**/*.css\" && cd apps/api && bun run lint && cd ../www && bun run lint",
    "start": "cd apps/api && bun start",
    "release": "node scripts/release-overwolf.mjs"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "stylelint": "15.6.1",
    "stylelint-config-standard": "33.0.0",
    "turbo": "1.11.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.0.16",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}