{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Aeternum Map-DEV",
    "author": "Leon Machens",
    "version": "3.20.0",
    "minimum-overwolf-version": "0.169.0",
    "description": "New World Interactive Map",
    "dock_button_title": "Aeternum Map",
    "icon": "icons/iconMouseOver.png",
    "icon_gray": "icons/iconMouseNormal.png",
    "launcher_icon": "icons/desktopIcon.ico",
    "window_icon": "icons/taskbarIcon.png"
  },
  "permissions": [
    "Hotkeys",
    "GameInfo",
    "Media",
    "FileSystem",
    "DesktopStreaming",
    "Clipboard"
  ],
  "data": {
    "start_window": "background",
    "windows": {
      "background": {
        "file": "background.html",
        "debug_url": "http://localhost:5173/background.html",
        "block_top_window_navigation": false,
        "is_background_page": true,
        "allow_local_file_access": true
      },
      "desktop": {
        "file": "index.html",
        "debug_url": "http://localhost:5173/index.html",
        "block_top_window_navigation": false,
        "resizable": true,
        "override_on_update": true,
        "desktop_only": true,
        "transparent": true,
        "native_window": true,
        "size": {
          "width": 1200,
          "height": 800
        },
        "min_size": {
          "width": 461,
          "height": 600
        },
        "allow_local_file_access": true,
        "popup_blocker": true,
        "mute": true,
        "keep_window_location": true,
        "disable_hardware_acceleration": true
      },
      "overlay": {
        "file": "index.html",
        "debug_url": "http://localhost:5173/index.html",
        "block_top_window_navigation": false,
        "resizable": true,
        "override_on_update": true,
        "transparent": true,
        "in_game_only": true,
        "focus_game_takeover": "ReleaseOnLostFocus",
        "focus_game_takeover_release_hotkey": "show_hide_app",
        "size": {
          "width": 1200,
          "height": 800
        },
        "min_size": {
          "width": 454,
          "height": 600
        },
        "allow_local_file_access": true,
        "popup_blocker": true,
        "mute": true
      },
      "minimap": {
        "file": "minimap.html",
        "debug_url": "http://localhost:5173/minimap.html",
        "block_top_window_navigation": false,
        "desktop_only": false,
        "in_game_only": true,
        "style": "inputPassThrough",
        "resizable": false,
        "transparent": true,
        "override_on_update": false,
        "size": {
          "width": 260,
          "height": 260
        },
        "start_position": {
          "top": 50,
          "left": 50
        },
        "allow_local_file_access": true,
        "popup_blocker": true,
        "mute": true,
        "keep_window_location": true,
        "restrict_to_game_bounds": true
      },
      "influence": {
        "file": "influence.html",
        "debug_url": "http://localhost:5173/influence.html",
        "block_top_window_navigation": false,
        "desktop_only": false,
        "in_game_only": true,
        "resizable": true,
        "transparent": true,
        "size": {
          "width": 642,
          "height": 640
        },
        "start_position": {
          "top": 100,
          "left": "100"
        },
        "allow_local_file_access": true,
        "topmost": true,
        "popup_blocker": true,
        "mute": true,
        "keep_window_location": true,
        "restrict_to_game_bounds": true
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [21816]
    },
    "game_events": [21816],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [21816]
        },
        "start_minimized": false
      }
    ],
    "hotkeys": {
      "show_hide_app": {
        "title": "Show/Hide App",
        "action-type": "custom",
        "default": "Ctrl+F1"
      },
      "show_hide_minimap": {
        "title": "Show/Hide Minimap",
        "action-type": "custom",
        "default": "Ctrl+F2"
      },
      "setup_minimap": {
        "title": "Setup Minimap",
        "action-type": "custom",
        "default": "Ctrl+F3"
      },
      "zoom_in_minimap": {
        "title": "Zoom In Minimap",
        "action-type": "custom",
        "default": "Ctrl+F4"
      },
      "zoom_out_minimap": {
        "title": "Zoom Out Minimap",
        "action-type": "custom",
        "default": "Ctrl+F5"
      },
      "zoom_in_map": {
        "title": "Zoom In Map (Website)",
        "action-type": "custom",
        "default": "F9"
      },
      "zoom_out_map": {
        "title": "Zoom Out Map (Website)",
        "action-type": "custom",
        "default": "F10"
      },
      "show_hide_direction": {
        "title": "Show/Hide Direction (Website)",
        "action-type": "custom",
        "default": "F11"
      },
      "show_hide_influence_overlay": {
        "title": "Show/Hide Influence Overlay",
        "action-type": "custom",
        "default": "Ctrl+F6"
      }
    },
    "url_protocol": {
      "scheme": "thgl-aeternum-map"
    },
    "protocol_override_domains": {
      "googlesyndication": "http"
    },
    "externally_connectable": {
      "matches": [
        "http://localhost:6006",
        "http://localhost:5173",
        "https://aeternum-map.th.gl"
      ]
    },
    "force_browser": "user",
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    },
    "extra-objects": {
      "discord": {
        "file": "plugins/DiscordRPCPlugin.dll",
        "class": "overwolf.plugins.DiscordRPCPlugin"
      }
    }
  }
}
